@model Dental.Models.DodajWizyteModel
    <div>
        @Html.Label("Gabinet:")<br />             
                @Html.DropDownListFor(model => model.GabinetID, Model.ListaGabinetow, "Wybierz", new { @class = "form-control" })
               <br />
    </div>

<script>
    $(document).ready(function () {

         $('#GabinetID').change(function () {
                debugger;
                var gabinet = $(this).val();
                $.ajax({
                    type: 'POST',
                    url: '/Pacjent/WizytaLekarz2',
                    data: { gabinet: gabinet },
                    success: function (data) {
                        $('#part3').html(data);
                    }

                });

            })
    })
</script>